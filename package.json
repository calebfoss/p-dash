{
  "name": "p5-marker",
  "version": "0.0.0",
  "description": "A p5 add-on to create sketches declaratively with XML",
  "type": "module",
  "main": "dist/p5.marker.js",
  "min": "dist/p5.marker.min.js",
  "targets": {
    "min": {
      "optimize": true
    }
  },
  "source": "src/index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "npx parcel build && node utils/addBuildComment.mjs ",
    "docs": "npx jsdoc -c docs/jsdoc.json dist -r",
    "test": "npm run build && npx mocha -r esm ./unit_tests",
    "watch": "npx parcel watch"
  },
  "keywords": [
    "creative",
    "coding",
    "p5",
    "processing"
  ],
  "author": "Caleb Foss",
  "license": "MIT",
  "devDependencies": {
    "chai": "x",
    "esm": "^3.2.25",
    "jsdoc": "^3.6.11",
    "mocha": "x",
    "p5": "x",
    "parcel": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/calebfoss/p5-marker.git"
  }
}
