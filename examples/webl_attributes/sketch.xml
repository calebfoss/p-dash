<?xml version="1.0" encoding="UTF-8"?>
<!-- https://p5js.org/reference/#/p5/setAttributes -->
<canvas width="100" height="100" background="255" renderer="WEBGL" twist="frame_count * 0.02"
    description="'A cube rotates demonstrating the antialias attribute of the 
    renderer being switch on and off'">
    <!-- Click to toggle antialias true/false -->
    <_ on="mouse_down"
        webgl_attributes="antialias: !webgl_attributes.antialias"></_>
    <box width="50" fill="0, 0, 0"
        angle_x="twist" angle_y="twist" angle_z="twist"></box>
</canvas>